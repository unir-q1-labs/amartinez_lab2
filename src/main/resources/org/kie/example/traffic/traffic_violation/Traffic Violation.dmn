      <dmn:contextEntry>
        <dmn:literalExpression id="_CA7CC7FE-C941-43E1-99B5-EEBA8FBA84DA">
          <dmn:text>if Total Points >= 20 then "Yes" else "No"</dmn:text>
        </dmn:literalExpression>
